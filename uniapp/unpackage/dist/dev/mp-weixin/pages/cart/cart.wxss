page {
	padding-bottom: 18%;
}
.goods {
	margin-top: 2%;
	margin-left: 2%;
	background-color: #fff;
	border-radius: 10rpx;
	width: 96%;
	height: 200rpx;
	display: flex;
	justify-content: space-between;
}
image {
	width: 40%;
	height: 100%;
	overflow: auto;
}
.count {
	flex-flow: row;
	justify-content: flex-end;
	display: flex;
}
.account {
	width: 100%;
	display: flex;
	justify-content: start;
	position: fixed;
	bottom: 0;
	background-color: #fff;
}

