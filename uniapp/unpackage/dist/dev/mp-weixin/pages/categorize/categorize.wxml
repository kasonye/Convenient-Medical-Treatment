<view class="classify"><view class="cate-left"><block wx:for="{{cateList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeActive',[index]]]]]}}" class="{{['cate-item',activeIndex==index?'active':'']}}" bindtap="__e">{{item.categoryName}}</view></block></view><view class="cate-right"><block wx:for="{{goodList}}" wx:for-item="it" wx:for-index="id" wx:key="id"><view data-event-opts="{{[['tap',[['nav',['$0',id],[[['goodList','',id]]]]]]]}}" class="cate-content" bindtap="__e"><image src="{{it.imageUrl}}" mode></image><view class="cate-text">{{it.medicineName+''}}</view></view></block></view></view>